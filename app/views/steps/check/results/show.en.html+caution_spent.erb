<% title t('.page_title') %>
<% track_transaction name: 'Caution check completed', category: 'Completed checks', dimensions: { spent: 'yes' } %>

<div class="govuk-width-container">
  <main class="govuk-main-wrapper" id="main-content">
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">

        <div class="govuk-panel govuk-panel--confirmation">
          <h1 class="govuk-panel__title">
            Your caution was spent on <span class="nowrap"><%= I18n.l(@presenter.expiry_date) %></span>
          </h1>
          <div class="govuk-panel__body">
            The following information will help you work out if you need to tell people about this caution.
          </div>
        </div>

        <h2 class="govuk-heading-m govuk-!-margin-top-7">
          What this result means
        </h2>

        <p class="govuk-body">
          Based on what you’ve told us, this caution is now spent.
        </p>

        <div class="govuk-inset-text">
          If you entered any approximate dates, the result you’ve been given will be incorrect.
        </div>

        <%= render partial: 'steps/check/results/shared/meaning' %>

        <%= render partial: 'steps/check/results/shared/summary', locals: { result: @presenter } %>

        <%= render partial: 'steps/check/results/shared/disclaimer' %>

        <%= render partial: 'steps/check/results/shared/feedback' %>
      </div>
    </div>
  </main>
</div>
